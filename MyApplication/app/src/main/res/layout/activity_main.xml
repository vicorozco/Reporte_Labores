<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteY="25dp">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="284dp"
        android:layout_height="69dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="68dp"
        android:layout_marginRight="68dp"
        app:srcCompat="@drawable/coopevictoria"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="69dp"
        android:layout_height="36dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="-8dp"
        android:text="Labor:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Labor:" />

    <Spinner
        android:id="@+id/spinner_labores"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="69dp"
        android:layout_height="36dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="55dp"
        android:text="Finca:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Finca:" />

    <Spinner
        android:id="@+id/spinner_fincas"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="69dp"
        android:layout_height="36dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="121dp"
        android:text="Lot:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Lote:" />

    <Spinner
        android:id="@+id/spinner_lotes"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="134dp"
        android:layout_height="37dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="190dp"
        android:text="Área trabajada:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Área trabajada:" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="60dp"
        android:layout_height="33dp"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="145dp"
        android:ems="10"
        android:inputType="number" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="78dp"
        android:layout_height="39dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="265dp"
        android:text="Fecha:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Fecha:" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="326dp"
        android:text="Encargado:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Encargado:" />

    <Spinner
        android:id="@+id/spinner_encargados"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="425dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="103dp" />

    <EditText
        android:id="@+id/editText4"
        style="?android:attr/datePickerStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/editText"
        android:layout_alignBottom="@+id/textView6"
        android:layout_marginEnd="-47dp"
        android:layout_marginBottom="-4dp"
        android:ems="10"
        android:hint="DD/MM/AAAA"
        android:inputType="date" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="56dp"
        android:text="Continuar"
        tools:text="Continuar" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="28dp"
        android:layout_height="33dp"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignBottom="@+id/textView5"
        android:layout_marginEnd="38dp"
        android:text="ha" />


</RelativeLayout>