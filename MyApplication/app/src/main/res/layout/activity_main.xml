<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteY="25dp">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="284dp"
        android:layout_height="69dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        app:srcCompat="@drawable/coopevictoria"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="69dp"
        android:layout_height="36dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="37dp"
        android:layout_marginLeft="37dp"
        android:layout_marginTop="-9dp"
        android:text="Labor:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Labor:" />

    <Spinner
        android:id="@+id/spinner_labores"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="29dp"
        android:layout_marginRight="29dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="69dp"
        android:layout_height="36dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="55dp"
        android:text="Finca:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Finca:" />

    <Spinner
        android:id="@+id/spinner_fincas"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="69dp"
        android:layout_height="36dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="130dp"
        android:text="Lot:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Lote:" />

    <Spinner
        android:id="@+id/spinner_lotes"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="134dp"
        android:layout_height="37dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="38dp"
        android:layout_marginLeft="38dp"
        android:layout_marginTop="190dp"
        android:text="Área trabajada:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Área trabajada:" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="140dp"
        android:layout_height="35dp"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="37dp"
        android:ems="10"
        android:inputType="number" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="78dp"
        android:layout_height="39dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="261dp"
        android:text="Fecha:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Fecha:" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_alignTop="@+id/spinner_labores"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="325dp"
        android:text="Encargado:"
        android:textSize="17sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="115dp"
        tools:text="Encargado:" />

    <Spinner
        android:id="@+id/spinner_encargados"
        android:layout_width="237dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="418dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="103dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/editText"
        android:layout_alignBottom="@+id/textView6"
        android:ems="10"
        android:inputType="date" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="Continuar"
        tools:text="Continuar" />





</RelativeLayout>